{
  "name": "sandbox-express",
  "version": "1.0.0",
  "description": "Sandbox Express",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node",
    "dev": "nodemon --exec npm run babel-node ./src/server/www",
    "start": "node dist/server/www",
    "build": "del-cli dist/ && mkdir dist && babel src -d dist",
    "db:init": "npm run db:delete && npm run db:populate",
    "db:delete": "del-cli db/",
    "db:populate": "npm run db:migrate && npm run db:seed",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all"
  },
  "keywords": [],
  "author": "Blas Medina <r.blas.m.c@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "del-cli": "^3.0.1",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.1",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0"
  }
}
