{
  "name": "sandbox-express",
  "version": "1.0.0",
  "description": "Sandbox Express",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "del-cli dist/ && mkdir dist && tsc",
    "start": "node ./dist",
    "start:dev": "ts-node ./src",
    "db:init": "npm run db:delete && npm run db:populate",
    "db:delete": "del-cli ./db/",
    "db:populate": "npm run db:migrate && npm run db:seed",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "lint": "eslint 'src/**/*.{js,ts}' --quiet",
    "ts-jest": "ts-jest",
    "test": "cross-env NODE_ENV=test jest --watchAll --testTimeout=10000"
  },
  "keywords": [],
  "author": "Blas Medina <r.blas.m.c@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/supertest": "^2.0.11",
    "axios": "^0.21.1",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "del-cli": "^3.0.1",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.0",
    "morgan": "^1.10.0",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/config": "0.0.38",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.2",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
